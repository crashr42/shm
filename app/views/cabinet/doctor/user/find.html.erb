<% @users.each do |u| %>
    <div class="user-info-block">
      <div class="pull-right">
        <%= link_to('Создать прием', "/cabinet/doctor/appointment/patient/#{u.id}/new", :class => 'btn') %>
      </div>
      <div class="pull-left">
        <div class="user-info">
          <%= u.first_name + ' ' + u.last_name + ' (' + u.email + ')'%>
        </div>
      </div>
    </div>
<% end %>
