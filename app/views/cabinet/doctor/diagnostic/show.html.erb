<div class="container-fluid">
    <div class="row-fluid">
        <div class="span3">
            <h3>Patient parameters</h3>
            <ul>
                <% @patient.parameters.each do |p| %>
                    <li>
                        <button class="parameter btn btn-link" data-id="<%= p.id %>"><%= p.name %></button>
                    </li>
                <% end %>
            </ul>
        </div>
        <div class="span9">
            <div class="parameter-diagnostic">

            </div>
        </div>
    </div>
</div>